<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.layout.*?>
<GridPane maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minWidth="-Infinity" prefWidth="450.0"
          xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1"
          fx:controller="controller.MainController">
    <columnConstraints>
        <ColumnConstraints hgrow="SOMETIMES" maxWidth="1.7976931348623157E308"/>
    </columnConstraints>
    <rowConstraints>
        <RowConstraints maxHeight="30.0" minHeight="30.0" prefHeight="30.0" vgrow="SOMETIMES"/>
        <RowConstraints maxHeight="60.0" minHeight="60.0" prefHeight="60.0" vgrow="SOMETIMES"/>
        <RowConstraints maxHeight="10.0" minHeight="10.0" prefHeight="10.0" vgrow="SOMETIMES"/>
        <RowConstraints maxHeight="30.0" minHeight="30.0" prefHeight="30.0" vgrow="SOMETIMES"/>
        <RowConstraints maxHeight="20.0" minHeight="20.0" prefHeight="20.0" vgrow="SOMETIMES"/>
        <RowConstraints minHeight="480.0" prefHeight="480.0" vgrow="SOMETIMES"/>
    </rowConstraints>
   <children>
      <MenuBar maxWidth="1.7976931348623157E308" minWidth="-Infinity">
        <menus>
          <Menu mnemonicParsing="false" text="File">
            <items>
                <MenuItem fx:id="menuAddList" mnemonicParsing="false" onAction="#addListActionListener"
                          text="Add list"/>
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Edit">
            <items>
                <MenuItem fx:id="undoButton" disable="true" mnemonicParsing="false" onAction="#undoActionListener"
                          text="Undo"/>
                <MenuItem fx:id="redoButton" disable="true" mnemonicParsing="false" onAction="#redoActionListener"
                          text="Redo"/>
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Help">
            <items>
              <MenuItem mnemonicParsing="false" text="About" />
            </items>
          </Menu>
        </menus>
      </MenuBar>
       <GridPane maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" GridPane.rowIndex="1">
        <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" maxWidth="20.0" minWidth="20.0" prefWidth="20.0" />
            <ColumnConstraints hgrow="SOMETIMES" minWidth="0.0" prefWidth="0.0" />
            <ColumnConstraints hgrow="SOMETIMES" maxWidth="360.0" minWidth="360.0" prefWidth="360.0" />
            <ColumnConstraints hgrow="SOMETIMES" minWidth="0.0" prefWidth="0.0" />
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="20.0" minWidth="20.0" prefWidth="20.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints maxHeight="20.0" minHeight="20.0" prefHeight="20.0" vgrow="SOMETIMES" />
            <RowConstraints maxHeight="40.0" minHeight="40.0" prefHeight="40.0" vgrow="SOMETIMES"/>
        </rowConstraints>
         <children>
            <GridPane GridPane.columnIndex="2" GridPane.rowIndex="1">
              <columnConstraints>
                  <ColumnConstraints hgrow="SOMETIMES" maxWidth="100.0" minWidth="40.0" prefWidth="40.0" />
                  <ColumnConstraints hgrow="SOMETIMES" maxWidth="40.0" minWidth="40.0" prefWidth="40.0" />
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="60.0" minWidth="60.0" prefWidth="60.0" />
                  <ColumnConstraints hgrow="SOMETIMES" maxWidth="60.0" minWidth="60.0" prefWidth="60.0" />
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="60.0" minWidth="60.0" prefWidth="60.0" />
                  <ColumnConstraints hgrow="SOMETIMES" maxWidth="40.0" minWidth="40.0" prefWidth="40.0" />
                  <ColumnConstraints hgrow="SOMETIMES" maxWidth="100.0" minWidth="40.0" prefWidth="40.0" />
              </columnConstraints>
              <rowConstraints>
                <RowConstraints maxHeight="40.0" minHeight="40.0" prefHeight="40.0" vgrow="SOMETIMES" />
              </rowConstraints>
               <children>
                  <Button fx:id="playButton" maxHeight="1.7976931348623157E308" mnemonicParsing="false" onAction="#playButtonActionListener" prefWidth="62.0" text="Button" GridPane.columnIndex="3" />
                  <Button fx:id="previousButton" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" onAction="#previousButtonActionListener" text="Button" GridPane.columnIndex="2" GridPane.halignment="CENTER">
                     <cursor>
                        <Cursor fx:constant="HAND" />
                     </cursor>
                  </Button>
                  <Button fx:id="nextButton" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" onAction="#nextButtonActionListener" text="Button" GridPane.columnIndex="4" GridPane.halignment="CENTER">
                     <cursor>
                        <Cursor fx:constant="HAND" />
                     </cursor>
                  </Button>
                  <Slider fx:id="volume" GridPane.columnIndex="6">
                     <cursor>
                        <Cursor fx:constant="HAND" />
                     </cursor>
                      <GridPane.margin>
                          <Insets left="5.0"/>
                      </GridPane.margin>
                  </Slider>
                   <ToggleButton fx:id="loopmodeButton" mnemonicParsing="false" onAction="#playmodeButtonActionListener"
                                 text="ToggleButton" GridPane.columnIndex="1"/>
                   <ToggleButton fx:id="playmodeButton" mnemonicParsing="false" onAction="#loopmodeButtonActionListener"
                                 text="ToggleButton" GridPane.columnIndex="5"/>
               </children>
            </GridPane>
         </children>
       </GridPane>
       <TabPane fx:id="playlistContainer" maxHeight="1.7976931348623157E308" minHeight="480.0" prefHeight="480.0"
                tabClosingPolicy="UNAVAILABLE" GridPane.rowIndex="5"/>
       <GridPane GridPane.rowIndex="3">
           <columnConstraints>
               <ColumnConstraints hgrow="SOMETIMES" maxWidth="50.0" minWidth="50.0" prefWidth="50.0"/>
               <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
               <ColumnConstraints hgrow="SOMETIMES" maxWidth="50.0" minWidth="50.0" prefWidth="50.0"/>
           </columnConstraints>
           <rowConstraints>
               <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
           </rowConstraints>
           <children>
               <Slider fx:id="progressBar" GridPane.columnIndex="1">
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor>
                   <GridPane.margin>
                       <Insets left="10.0" right="10.0"/>
                   </GridPane.margin>
            </Slider>
               <Label fx:id="currentTime" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
                      nodeOrientation="RIGHT_TO_LEFT" text="Label"/>
               <Label fx:id="totalLength" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
                      text="Label" GridPane.columnIndex="2">
                   <GridPane.margin>
                       <Insets/>
                   </GridPane.margin>
               </Label>
         </children>
      </GridPane>
   </children>
</GridPane>
